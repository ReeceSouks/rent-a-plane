<div class="container pt-5 pb-5">
  <h2 class="text-center"><strong>What can we help you find, <%= @user.first_name %> ?</strong></h2>
    <div class="d-flex justify-content-center pt-5 flex-column align-items-center">
      <div class="card-trip text-left mb-6">
        <img src="data:image/jpeg;base64," />
      <div class="card-trip-infos text-left pd-5 mb-6">
    <div>
    <h4><strong>First Name:</strong> <%= @user.first_name %></h4>
    <h4><strong>Last Name:</strong> <%= @user.last_name %></h4>
    <h4><strong>Email:</strong> <%= @user.email %></h4>
    </div>
  </div>

    </div>
    </div>
    <div class="d-flex justify-content-center pt-5 flex-column align-items-center">
      <h3 class="text-center">My airplanes:</h3>
      <% @airplanes.each do |airplane| %>
        <%=link_to airplane.title , airplane_path(airplane) %>
      <% end %>
      <h3 class="text-center">My bookings:</h3>
      <ul class="pb-5 px-0 d-flex flex-wrap justify-content-center">
      <% @bookings.each do |booking| %>
        <div class="text-center row mr-5">
          <div class="card-trip" id="booking-show">
            <h2 style="font-size: 17px"><strong>My Upcoming Flight:</strong></h2>
            <div class="booking-show-p">
              <p>From: <%= booking.start_date %></p>
              <p>To: <%= booking.end_date %></p>
              <p>Plane id: <%= booking.airplane.id %></p>
              <p><strong>Total price: $</strong><%= booking.price %></p>
            </div>
          </div>
        </div>
      <% end %>
      </ul>
    </div>
</div>



